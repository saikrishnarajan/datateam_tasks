select * from T_User(nolock) where Email='kristen.mclain@nbfc.com'--6617


select * from T_SourceType(nolock) where CompanyId=6617 and Name NOT IN  ('Indeed','Glassdoor','LinkedinFree')

select * from T_SourceType where CompanyId=6617 and Name = 'Job Board';/*637506*/



--Main Query
Update T_SourceType SET Deleted=1, modifieddate=GETDATE() where CompanyId=6617 and Name NOT IN ('Indeed','Glassdoor','LinkedinFree’);

Update T_SourceType Set Deleted=0, modifieddate=GETDATE() where CompanyId=6617 and Id=637506;

--Rollback Query
Update T_SourceType SET Deleted=0, modifieddate=GETDATE() where CompanyId=6617 and Name NOT IN ('Indeed','Glassdoor','LinkedinFree’);

Update T_SourceType Set Deleted=1, modifieddate=GETDATE() where CompanyId=6617 and Id=637506;