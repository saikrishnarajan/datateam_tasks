select * from T_Company (nolock) where name like '%Victorian Order of Nurses%'--6288


select * from T_Form (nolock) where CompanyId =6288



select PreInterviewFormId,* from T_Job (nolock) where CompanyId =6288 and Deleted = 0 and PreInterviewFormId =43171

--update script:- 
update T_Form set IsDeleted = 1, modifieddate = getdate() where CompanyId = 6288 and id = 43171

update T_Job set PreInterviewFormId = 0, Modified = getdate() where CompanyId = 6288  and Deleted = 0 and PreInterviewFormId =43171 and id in (3137203,3138116,3144446,3148019,3153279,3154516,3155079,3164346,3165666,3171154,3174492,3197297,3197316,3197330,3197341,3197370,3199562,3199601,3199632,3204963,3204984,3208918,3208940,3208953,3208960,3213102,3215954,3215965,3218637,3222683,3240019,3243049,3245823,3247608,3249392,3250011,3255086,3261943,3261990,3266812,3266825,3267258,3271824,3271843,3273995,3276929,3278491,3283602,3284164,3286199,3292197,3300842,3300858,3300889,3300897,3300929,3300954,3302415,3302416,3302423,3302435,3302439,3302445,3306896,3307561,3310952,3332403,3341592,3388973,3459786,3469081)

--Rollback:-
update T_Form set IsDeleted = 0, modifieddate = getdate() where CompanyId = 6288 and id = 43171

update T_Job set PreInterviewFormId = 43171, Modified = getdate() where CompanyId = 6288  and Deleted = 0 and id in (3137203,3138116,3144446,3148019,3153279,3154516,3155079,3164346,3165666,3171154,3174492,3197297,3197316,3197330,3197341,3197370,3199562,3199601,3199632,3204963,3204984,3208918,3208940,3208953,3208960,3213102,3215954,3215965,3218637,3222683,3240019,3243049,3245823,3247608,3249392,3250011,3255086,3261943,3261990,3266812,3266825,3267258,3271824,3271843,3273995,3276929,3278491,3283602,3284164,3286199,3292197,3300842,3300858,3300889,3300897,3300929,3300954,3302415,3302416,3302423,3302435,3302439,3302445,3306896,3307561,3310952,3332403,3341592,3388973,3459786,3469081)
