/*MySql Update Query*/
Update t_application Set DeletedHM=1, Modified=CURRENT_TIMESTAMP() where CompanyId=6200 and CandidateId=62002311;
/*Sql Update Query*/
Update t_application Set DeletedHM=1, Modified=GETDATE() where CompanyId=6200 and CandidateId=62002311;


/*MySql Update Query*/
Update t_application Set DeletedHM=0, Modified=CURRENT_TIMESTAMP() where CompanyId=6200 and CandidateId=62002311;
/*Sql Update Query*/
Update t_application Set DeletedHM=0, Modified=GETDATE() where CompanyId=6200 and CandidateId=62002311;
